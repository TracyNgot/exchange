    "i18next": "^19.3.3",
    "i18next-browser-languagedetector": "^4.0.2",
    "lodash": "^4.17.15",
    "netlify-lambda": "^1.6.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "serverless-http": "^2.3.2",
    "source-map-explorer": "^2.4.1",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "netlify deploy",
    "start.server": "nodemon --config nodemon.json",
    "start": "react-scripts start",
    "build": "npm-run-all build.**",
    "build.antd": "yarn lessc --js src/antd.less src/antd.css",
    "build.app": "react-scripts build",
    "build.server": "netlify-lambda build server/",
    "test": "react-scripts test",
    "test.ci": "CI=true TZ=UTC yarn test --coverage --ci --maxWorkers=2 -u",
    "test.server": "jest --rootDir server/ --config server/jest.config.test.js",
    "test.server.ci": "CI=true TZ=UTC yarn test.server --coverage --ci --maxWorkers=2 -u",
    "eject": "react-scripts eject",
    "lint": "yarn lint.ci --fix",
    "lint.ci": "eslint src --ext .ts,.tsx",
    "preanalyze": "yarn build",
    "prebuild": "rimraf lib/ build/",
    "predeploy": "yarn build",
    "prestart": "yarn build.antd"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/i18n/index.ts",
      "src/store/create-store.ts",
      "src/setupProxy.js",
      "src/test/",
      "src/store/"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/i18next": "^13.0.0",
    "@types/lodash": "^4.14.149",
    "@types/react-i18next": "^8.1.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "encoding": "^0.1.12",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0"
  }
}
